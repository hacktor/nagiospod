postfix postfix/main_mailer_type string "'Internet Site'"
postfix postfix/mynetworks string "127.0.0.0/8"
postfix postfix/mailname string "nagioshack.hacktor.com"

tzdata tzdata/Areas select Europe
tzdata tzdata/Zones/Europe select Amsterdam

nagios3-cgi nagios3/adminpassword-repeat password huppeldepup
nagios3-cgi nagios3/adminpassword password huppeldepup
nagios3-cgi nagios3/adminpassword-mismatch note
